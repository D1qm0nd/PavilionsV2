<UserControl x:Class="WPFUserControls.UserControls.PasswordDifficultyChecker"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFUserControls"
             mc:Ignorable="d" 
             Height="60" Width="200">
    <Border Background="WhiteSmoke" BorderBrush="DarkSlateGray" BorderThickness="2" CornerRadius="15">
        <StackPanel>
            <Label HorizontalContentAlignment="Center" >Сложность пароля:</Label>
            <Label x:Name="Difficulty" HorizontalContentAlignment="Center"></Label>
        </StackPanel>
    </Border>
</UserControl>
